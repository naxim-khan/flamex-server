    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-thermal-printer": "^4.5.0",
    "pg": "^8.16.3",
    "react-icons": "^5.5.0",
    "redis": "^5.10.0",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
